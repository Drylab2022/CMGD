{
  "hosting": {
    "S3AndCloudFront": {
      "service": "S3AndCloudFront",
      "providerPlugin": "awscloudformation"
    }
  },
  "auth": {
    "metagenomiccurations": {
      "service": "Cognito",
      "serviceType": "managed",
      "providerPlugin": "awscloudformation",
      "dependsOn": [],
      "frontendAuthConfig": {
        "socialProviders": [],
        "usernameAttributes": [],
        "signupAttributes": [
          "EMAIL"
        ],
        "passwordProtectionSettings": {
          "passwordPolicyMinLength": 8,
          "passwordPolicyCharacters": []
        },
        "mfaConfiguration": "OFF",
        "mfaTypes": [
          "SMS"
        ],
        "verificationMechanisms": [
          "EMAIL"
        ]
      }
    }
  },
  "api": {
    "metagenomiccuration": {
      "resourceName": "metagenomiccuration",
      "dependsOn": [
        {
          "category": "",
          "resourceName": "NetworkStack",
          "attributes": [
            "ClusterName",
            "VpcId",
            "VpcCidrBlock",
            "SubnetIds",
            "VpcLinkId",
            "CloudMapNamespaceId"
          ]
        },
        {
          "category": "auth",
          "resourceName": "metagenomiccurations",
          "attributes": [
            "UserPoolId",
            "AppClientIDWeb"
          ]
        }
      ],
      "deploymentMechanism": "FULLY_MANAGED",
      "imageSource": {
        "type": "TEMPLATE",
        "template": "dockerfile-rest-express"
      },
      "restrictAccess": true,
      "build": true,
      "providerPlugin": "awscloudformation",
      "service": "ElasticContainer",
      "authName": "metagenomiccurations",
      "skipHashing": false,
      "apiType": "REST",
      "iamAccessUnavailable": true
    }
  }
}